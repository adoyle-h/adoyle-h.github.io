  "private": true,
  "main": "index.js",
  "bugs": {
    "url": "https://github.com/adoyle-h/adoyle-h.github.io/issues"
  },
  "homepage": "https://github.com/adoyle-h/adoyle-h.github.io#readme",
  "scripts": {
    "fix": "npm-run-all fix:*",
    "fix:eslint": "rm -rf ./node_modules/razzle/node_modules/eslint",
    "start": "PORT=${PORT:-3000} npm-run-all -p start:razzle open",
    "open": "open-cli http://localhost:${PORT:-3000}",
    "start:razzle": "razzle start",
    "build": "razzle build",
    "clean:build": "rm -rf ./build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js"
  },
  "engines": {
    "node": ">=10.15.0"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "maintained node versions",
    "not ie < 11"
  ],
  "dependencies": {
    "@adoyle.me/winston-logger": "^0.1.2",
    "object-hash": "^1.3.1",
    "toml": "^3.0.0",
    "fast-glob": "^2.2.6",
    "js-yaml": "^3.13.1",
    "loader-utils": "^1.2.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {}
}
